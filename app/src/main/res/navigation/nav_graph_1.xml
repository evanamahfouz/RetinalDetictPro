<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_1"
    app:startDestination="@id/dashBoradFrag">
    <fragment
        android:id="@+id/dashBoradFrag"
        android:name="com.example.ratinadeticpro.Ui.ui.Dashboard_Main.DashBoradFrag"
        android:label="fragment_dash_borad_"
        tools:layout="@layout/fragment_dash_borad_">
        <action
            android:id="@+id/action_dashBoradFrag_to_predictFragment2"
            app:destination="@id/predictFragment2" />
        <action
            android:id="@+id/action_dashBoradFrag_to_chartFragment"
            app:destination="@id/dashboardFragment2" />
        <action
            android:id="@+id/action_dashBoradFrag_to_historyFragment"
            app:destination="@id/historyFragment" />
        <action
            android:id="@+id/action_dashBoradFrag_to_profileFragment"
            app:destination="@id/profileFragment" />
        <action
            android:id="@+id/action_dashBoradFrag_to_aboutUsFragment"
            app:destination="@id/aboutUsFragment" />
    </fragment>
    <fragment
        android:id="@+id/predictFragment2"
        android:name="com.example.ratinadeticpro.Ui.ui.predict.PredictFragment"
        android:label="predict_fragment"
        tools:layout="@layout/predict_fragment">
        <action
            android:id="@+id/action_predictFragment2_to_resultFragment2"
            app:destination="@id/resultFragment2" />
        <action
            android:id="@+id/action_predictFragment2_to_dashBoradFrag"
            app:destination="@id/dashBoradFrag" />
    </fragment>
    <fragment
        android:id="@+id/resultFragment2"
        android:name="com.example.ratinadeticpro.Ui.ui.RsultFragment.ResultFragment"
        android:label="fragment_result"
        tools:layout="@layout/fragment_result" />
    <fragment
        android:id="@+id/dashboardFragment2"
        android:name="com.example.ratinadeticpro.Ui.ui.Chart.ChartFragment"
        android:label="dashboard_fragment"
        tools:layout="@layout/dashboard_fragment" />
    <fragment
        android:id="@+id/historyFragment"
        tools:layout="@layout/fragment_history"
        android:name="com.example.ratinadeticpro.Ui.ui.History.HistoryFragment"
        android:label="HistoryFragment" >
        <action
            android:id="@+id/action_historyFragment_to_dashBoradFrag"
            app:destination="@id/dashBoradFrag" />
    </fragment>
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.example.ratinadeticpro.Ui.ui.Profile.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" >
        <action
            android:id="@+id/action_profileFragment_to_dashBoradFrag"
            app:destination="@id/dashBoradFrag" />
    </fragment>
    <fragment
        android:id="@+id/aboutUsFragment"
        android:name="com.example.ratinadeticpro.Ui.ui.AboutUs.AboutUsFragment"
        android:label="fragment_about_us"
        tools:layout="@layout/fragment_about_us" >
        <action
            android:id="@+id/action_aboutUsFragment_to_dashBoradFrag"
            app:destination="@id/dashBoradFrag" />
    </fragment>
</navigation>