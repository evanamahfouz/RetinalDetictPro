<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/predictFragment">


    <fragment
        android:id="@+id/predictFragment"
        android:name="com.example.ratinadeticpro.Ui.ui.predict.PredictFragment"
        android:label="PredictFragment"
        tools:layout="@layout/predict_fragment">
        <action
            android:id="@+id/action_predictFragment_to_dashboardFragment"
            app:destination="@id/dashboardFragment" />
        <action
            android:id="@+id/action_predictFragment_to_resultFragment"
            app:destination="@id/resultFragment" />
    </fragment>
    <fragment
        android:id="@+id/dashboardFragment"
        android:name="com.example.ratinadeticpro.Ui.ui.DashBoard.DashboardFragment"
        android:label="dashboard_fragment"
        tools:layout="@layout/dashboard_fragment" />
    <fragment
        android:id="@+id/resultFragment"
        android:name="com.example.ratinadeticpro.Ui.ui.RsultFragment.ResultFragment"
        android:label="fragment_result"
        tools:layout="@layout/fragment_result" />
</navigation>